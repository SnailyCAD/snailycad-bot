{
  "name": "discord-bot-template",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "npx prisma generate && npx swc ./src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test-tsc": "tsc --noEmit",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@prisma/client": "^4.0.0",
    "discord.js": "^14.0.0-dev.1654519696-fba9710",
    "globby": "^13.1.2",
    "undici": "^5.6.0"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^4.4.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.208",
    "@swc/helpers": "^0.4.3",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
