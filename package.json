{
  "name": "snailycad-bot",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npx prisma db push && nodemon",
    "build": "npx prisma generate && npx swc ./src -d dist",
    "start": "npx prisma migrate deploy && node dist/index.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test-tsc": "tsc --noEmit",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.5",
    "@prisma/client": "^5.5.2",
    "@sentry/node": "^7.76.0",
    "discord.js": "^14.13.0",
    "globby": "^13.2.2",
    "pretty-ms": "^8.0.0",
    "undici": "^5.27.0"
  },
  "devDependencies": {
    "@casperiv/eslint-config": "^8.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.100",
    "@swc/helpers": "^0.5.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.8.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "regenerator-runtime": "^0.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.0.1"
}
