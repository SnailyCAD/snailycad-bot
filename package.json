{
  "name": "snailycad-bot",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npx prisma db push && nodemon",
    "build": "npx prisma generate && npx swc ./src -d dist",
    "start": "npx prisma migrate deploy && node dist/index.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test-tsc": "tsc --noEmit",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.3",
    "@prisma/client": "^4.15.0",
    "@sentry/node": "^7.54.0",
    "discord.js": "^14.11.0",
    "globby": "^13.1.4",
    "pretty-ms": "^8.0.0",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^6.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.61",
    "@swc/helpers": "^0.5.1",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.2.5",
    "dotenv": "^16.1.3",
    "eslint": "^8.41.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.5.1"
}
